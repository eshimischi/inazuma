{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "build:renderer": "webpack --config src/renderer/webpack.config.js",
    "build:browser": "webpack --config src/browser/webpack.config.js",
    "build:style": "webpack --config webpack.config.style.js",
    "prettier": "prettier src/**/*.ts --write",
    "prettier-vue": "node ./prettier-vue.js src/renderer/**/*.vue",
    "format": "npm-run-all prettier prettier-vue",
    "lint": "eslint src/**/*.{ts,vue}",
    "watch": "webpack -w",
    "watch:renderer": "webpack -w --config src/renderer/webpack.config.js",
    "watch:browser": "webpack -w --config src/browser/webpack.config.js",
    "watch:style": "webpack -w --config webpack.config.style.js",
    "start": "electron ."
  },
  "dependencies": {
    "electron": "1.7.9",
    "fs-extra": "^5.0.0",
    "ipc-promise": "^0.1.4",
    "jsonschema": "^1.2.2",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.19.4",
    "rimraf": "^2.6.2",
    "sinai": "^0.1.7",
    "vue": "^2.5.9",
    "vue-class-component": "^6.1.1",
    "vue-material": "1.0.0-beta-7",
    "vue-router": "^3.0.1",
    "vue-strict-prop": "^0.1.1",
    "vue-template-compiler": "^2.5.9",
    "vue-tsx-support": "^1.2.0",
    "vue-typed-component": "^0.10.1",
    "vue-vtable": "^0.9.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^4.0.6",
    "@types/lodash": "^4.14.88",
    "@types/rimraf": "^2.0.2",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0-beta.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "sass-loader": "^6.0.6",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^11.0.0",
    "vue-css-definition": "^0.1.0",
    "vue-loader": "^13.5.0",
    "vue-template-compiler-loader": "^1.0.4",
    "webpack": "^3.10.0"
  }
}
