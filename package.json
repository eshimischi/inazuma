{
  "name": "inazuma",
  "version": "0.2.2",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules/.cache dist/* src/react/generated/* src-tauri/target",
    "prebuild:iconbundle": "ts-node build/create-iconbundle.ts",
    "generate-dts:bindings": "ts-node build/generate-bindings.ts",
    "generate-dts:invoke": "ts-node build/generate-invoke.ts",
    "generate-dts": "run-s generate-dts:*",
    "format": "prettier --write \"src/react/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/react/**/*.{ts,tsx}",
    "check:ts": "tsc -p src/react/tsconfig.json --noEmit",
    "check:tauri": "cargo check --manifest-path src-tauri/app/Cargo.toml",
    "build:tailwind": "tailwindcss -i ./src/react/index.css -o ./src/react/generated/tailwind.css",
    "build:ts": "cross-env NODE_OPTIONS=\"--max_old_space_size=3072\" vite build",
    "build:frontend": "run-s prebuild:* generate-dts check:ts build:tailwind build:ts",
    "build:tauri": "tauri build",
    "test:tauri": "cargo test --manifest-path src-tauri/app/Cargo.toml",
    "build": "run-s build:frontend build:tauri",
    "watch:ts": "vite",
    "watch:frontend": "run-p \"build:tailwind -w\" \"check:ts -w\" watch:ts",
    "watch:tauri": "tauri dev",
    "watch": "run-p watch:*",
    "all": "run-s clean format lint build"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.1.0",
    "buffer": "^6.0.3",
    "chardet": "^1.3.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.5",
    "iconv-lite": "^0.6.2",
    "lodash": "^4.17.20",
    "monaco-editor": "^0.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-redux": "^8.0.4",
    "react-transition-group": "^4.4.5",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.7",
    "string_decoder": "^1.3.0",
    "xterm": "^5.0.0",
    "xterm-addon-fit": "^0.6.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@iconify/json": "^2.1.113",
    "@iconify/react": "^4.0.0",
    "@iconify/utils": "^2.0.0",
    "@mui/material": "^5.10.6",
    "@mui/styles": "^5.10.6",
    "@reduxjs/toolkit": "^1.8.5",
    "@tauri-apps/cli": "^1.1.1",
    "@types/chardet": "^0.8.0",
    "@types/lodash": "^4.14.185",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fibers": "^5.0.3",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "npm-run-all": "^4.1.5",
    "postcss": "~8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "upath": "^2.0.1",
    "vite": "3.2.7"
  }
}
