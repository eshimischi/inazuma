{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:renderer build:browser",
    "build:renderer": "webpack --config src/renderer/webpack.config.js",
    "build:browser": "tsc -p src/browser/tsconfig.json",
    "clean": "rimraf dist src/renderer/.cache-loader",
    "format": "prettier --write \"src/**/*.{ts,js,vue,css,scss}\"",
    "lint": "eslint src/**/*.{ts,vue}",
    "postinstall": "npm run clean",
    "watch": "npm-run-all --parallel watch:renderer watch:browser",
    "watch:renderer": "webpack -w --config src/renderer/webpack.config.js",
    "watch:browser": "tsc -w -p src/browser/tsconfig.json",
    "start": "electron ."
  },
  "dependencies": {
    "electron": "1.8.4",
    "fs-extra": "^5.0.0",
    "ipc-promise": "^0.1.4",
    "jsonschema": "^1.2.2",
    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.0",
    "monaco-editor": "^0.12.0",
    "rimraf": "^2.6.2",
    "sinai": "^0.1.7",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-material": "1.0.0-beta-8",
    "vue-monaco": "^0.1.6",
    "vue-router": "^3.0.1",
    "vue-strict-prop": "^0.1.1",
    "vue-template-compiler": "^2.5.16",
    "vue-tsx-support": "^1.2.0",
    "vue-typed-component": "^0.10.1",
    "vue-vtable": "^0.10.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.1",
    "@types/lodash": "^4.14.106",
    "@types/rimraf": "^2.0.2",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-plugin-vue-jsx-modifier": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "cache-loader": "^1.2.2",
    "chalk": "^2.3.2",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "glob": "^7.1.2",
    "node-sass": "4.7.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass-loader": "^6.0.7",
    "ts-loader": "^3.3.1",
    "typescript": "^2.8.1",
    "typescript-eslint-parser": "^14.0.0",
    "vue-css-definition": "^0.1.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler-loader": "^1.0.4",
    "webpack": "^3.10.0"
  }
}
