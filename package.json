{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "prebuild:style": "node-sass src/renderer/style/application.scss dist/renderer/application.css",
    "prebuild:copy": "node build/copyFiles.js",
    "prebuild": "run-s prebuild:*",
    "build:renderer": "webpack --config src/renderer/webpack.config.js",
    "build:browser": "tsc -p src/browser/tsconfig.json",
    "build": "run-s build:renderer build:browser",
    "watch:renderer": "webpack --config src/renderer/webpack.config.js -w",
    "watch:browser": "tsc -p src/browser/tsconfig.json -w",
    "watch": "run-p watch:*",
    "clean": "rimraf dist out src/renderer/.cache-loader src/renderer/**/__generated",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "postinstall": "rimraf src/renderer/.cache-loader",
    "package": "electron-builder",
    "dev": "run-s prebuild watch",
    "all": "run-s clean format lint build package",
    "start": "electron ."
  },
  "dependencies": {
    "chardet": "^0.7.0",
    "fs-extra": "^7.0.0",
    "iconv-lite": "^0.4.24",
    "ipc-promise": "^0.1.4",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "@types/chardet": "^0.5.0",
    "@types/fs-extra": "^5.0.4",
    "@types/iconv-lite": "^0.0.1",
    "@types/lodash": "^4.14.117",
    "@types/rimraf": "^2.0.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "babel-plugin-vue-jsx-modifier": "^1.0.0",
    "cache-loader": "^1.2.2",
    "cpx": "^1.5.0",
    "electron": "3.0.4",
    "electron-builder": "^20.28.4",
    "emotion": "^9.2.12",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "^0.4.10",
    "glob": "^7.1.3",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "monaco-editor": "^0.13.1",
    "node-sass": "4.9.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "sinai": "^0.2.2",
    "ts-loader": "^5.2.2",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^17.0.1",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-css-definition": "^0.1.0",
    "vue-material": "1.0.0-beta-10.2",
    "vue-monaco": "^0.1.6",
    "vue-strict-prop": "^0.3.1",
    "vue-template-compiler": "^2.5.17",
    "vue-tsx-support": "^2.2.0",
    "vue-typed-component": "^0.11.0",
    "vue-vtable": "^0.14.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
