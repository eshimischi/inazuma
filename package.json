{
  "name": "inazuma",
  "version": "0.0.1",
  "main": "dist/browser/main.js",
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "scripts": {
    "prebuild:style": "node-sass src/renderer/style/application.scss dist/renderer/application.css",
    "prebuild:copy": "node build/copyFiles.js",
    "prebuild": "run-s prebuild:*",
    "build:renderer": "webpack --config src/renderer/webpack.config.js",
    "build:browser": "tsc -p src/browser/tsconfig.json",
    "build": "run-s build:renderer build:browser",
    "watch:renderer": "webpack --config src/renderer/webpack.config.js -w",
    "watch:browser": "tsc -p src/browser/tsconfig.json -w",
    "watch": "run-p watch:*",
    "clean": "rimraf dist out src/renderer/.cache-loader src/renderer/**/__generated",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "postinstall": "rimraf src/renderer/.cache-loader",
    "package": "electron-builder",
    "dev": "run-s prebuild watch",
    "all": "run-s clean format lint build package",
    "start": "electron ."
  },
  "dependencies": {
    "chardet": "^0.5.0",
    "fs-extra": "^6.0.1",
    "iconv-lite": "^0.4.23",
    "ipc-promise": "^0.1.4",
    "jsonschema": "^1.2.4",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@types/chardet": "^0.5.0",
    "@types/fs-extra": "^5.0.2",
    "@types/iconv-lite": "^0.0.1",
    "@types/lodash": "^4.14.109",
    "@types/rimraf": "^2.0.2",
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-plugin-vue-jsx-modifier": "^0.2.0",
    "babel-preset-env": "^1.7.0",
    "cache-loader": "^1.2.2",
    "copy-webpack-plugin": "^4.5.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "electron": "2.0.1",
    "electron-builder": "^20.13.4",
    "emotion": "^9.2.6",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "glob": "^7.1.2",
    "lodash": "^4.17.10",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.1",
    "monaco-editor": "^0.13.1",
    "node-sass": "4.9.0",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "sass-loader": "^7.0.1",
    "sinai": "^0.1.7",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^15.0.0",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-css-definition": "^0.1.0",
    "vue-material": "1.0.0-beta-10.2",
    "vue-monaco": "^0.1.6",
    "vue-strict-prop": "^0.2.0",
    "vue-template-compiler": "^2.5.16",
    "vue-tsx-support": "^2.1.1",
    "vue-typed-component": "^0.11.0",
    "vue-vtable": "^0.14.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  }
}
